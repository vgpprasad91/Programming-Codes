// I completed this program using a single for loop

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
                Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int[] a = new int[n];
        for(int i=0; i <n; i++){
            a[n-i-1] = in.nextInt();
        }
        System.out.print(Arrays.toString(a).replaceAll("[\\[\\]]", "").replaceAll(", ", " "));
    }
    
}
